{
	"devDependencies": {
		"@electron/typescript-definitions": "^8.15.0",
		"@mxssfd/typedoc-theme": "^1.1.3",
		"concurrently": "^8.2.1",
		"electron": "^27.0.1",
		"esbuild": "0.19.3",
		"tsup": "^7.2.0",
		"typedoc": "^0.25.2",
		"vitest": "^0.34.6"
	},
	"scripts": {
		"dev": "tsup --sourcemap --metafile --watch --dts src/engine/index.ts",
